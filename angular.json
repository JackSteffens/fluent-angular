            "tsConfig": "components/icon/lib/tsconfig.lib.json",
            "project": "components/icon/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "components/icon/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "components/icon/src/test.ts",
            "tsConfig": "components/icon/tsconfig.spec.json",
            "karmaConfig": "components/icon/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "components/icon/tsconfig.lib.json",
              "components/icon/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "reveal": {
      "root": "components/reveal",
      "sourceRoot": "components/reveal/src",
      "projectType": "library",
      "prefix": "fluent",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "components/reveal/tsconfig.lib.json",
            "project": "components/reveal/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "components/reveal/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "components/reveal/src/test.ts",
            "tsConfig": "components/reveal/tsconfig.spec.json",
            "karmaConfig": "components/reveal/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "components/reveal/tsconfig.lib.json",
              "components/reveal/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "check-box": {
      "root": "components/check-box",
      "sourceRoot": "components/check-box/src",
      "projectType": "library",
      "prefix": "fluent",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "components/check-box/tsconfig.lib.json",
            "project": "components/check-box/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "components/check-box/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "components/check-box/src/test.ts",
            "tsConfig": "components/check-box/tsconfig.spec.json",
            "karmaConfig": "components/check-box/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "components/check-box/tsconfig.lib.json",
              "components/check-box/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "fluent-demo",
  "cli": {
    "analytics": "237b6945-427f-412c-974c-a6ea38631f92"
  }
}
