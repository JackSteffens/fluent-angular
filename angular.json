{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "button": {
      "root": "components/button",
      "sourceRoot": "components/button/src",
      "projectType": "library",
      "prefix": "fluent",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "components/button/tsconfig.lib.json",
            "project": "components/button/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "components/button/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "components/button/src/test.ts",
            "tsConfig": "components/button/tsconfig.spec.json",
            "karmaConfig": "components/button/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "components/button/tsconfig.lib.json",
              "components/button/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "fluent-demo": {
      "root": "fluent-demo/",
      "sourceRoot": "fluent-demo/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "outputPath": "dist/fluent-demo",
            "index": "fluent-demo/src/index.html",
            "main": "fluent-demo/src/main.ts",
            "polyfills": "fluent-demo/src/polyfills.ts",
            "tsConfig": "fluent-demo/tsconfig.app.json",
            "assets": [
              "fluent-demo/src/favicon.ico",
              "fluent-demo/src/assets"
            ],
            "styles": [
              "fluent-demo/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "fluent-demo/src/environments/environment.ts",
                  "with": "fluent-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "fluent-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "fluent-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fluent-demo:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "fluent-demo/src/test.ts",
            "polyfills": "fluent-demo/src/polyfills.ts",
            "tsConfig": "fluent-demo/tsconfig.spec.json",
            "karmaConfig": "fluent-demo/karma.conf.js",
            "styles": [
              "fluent-demo/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "fluent-demo/src/favicon.ico",
              "fluent-demo/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "fluent-demo/tsconfig.app.json",
              "fluent-demo/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "fluent-demo-e2e": {
      "root": "fluent-demo-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "fluent-demo-e2e/protractor.conf.js",
            "devServerTarget": "fluent-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fluent-demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "fluent-demo-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "icon": {
      "root": "components/icon",
      "sourceRoot": "components/icon/src",
      "projectType": "library",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
